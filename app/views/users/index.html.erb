<table class="seed-admin">
	<tr>
		<th>Name</th>
		<th>Login</th>
		<th>Email</th>
		<th>Active?</th>
		<th>Roles</th>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td><strong><%= user.name %></strong></td>
		<td><%= user.login %></td>
		<td><%= user.email %></td>
		<td><%= user.active? %></td>
		<td>
			<% user.roles.each do |role| %>
				<%= "#{role.name} " %>
			<% end -%>
		</td>
	</tr>
	<% end -%>
</table>
